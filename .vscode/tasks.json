{
  "version": "0.1.0",
  "command": "yarn",
  "isShellCommand": true,
  "showOutput": "always",
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "install",
      "args": [ "install" ]
    },
    {
      "taskName": "build",
      "args": [ "run", "build" ],
      "isBuildCommand": true
    }
  ]
}

